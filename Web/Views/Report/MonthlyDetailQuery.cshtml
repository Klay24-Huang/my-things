@model  IEnumerable<Domain.TB.BackEnd.BE_MonthlyReportData>
@{
    int subscriptReportLen = 0;
    List<Domain.TB.BackEnd.BE_MonthlyReportData> subscriptReportData = new List<Domain.TB.BackEnd.BE_MonthlyReportData>();
    //var ticket = (FormsIdentity)User.Identity;
    if (null == Model)
    {
        subscriptReportLen = 0;
    }
    else
    {
        subscriptReportData = Model.ToList<Domain.TB.BackEnd.BE_MonthlyReportData> ();
        subscriptReportLen = subscriptReportData.Count();

    }

}
<!--查詢介面-->
<div class="container mt-4">
    <!--card start-->
    <div class="card">
        <div class="card-header bg-secondary text-white">
            月租報表
        </div>

        <ul class="list-group list-group-flush">
            <!--每一個li是一項查詢條件-->
            @using (Html.BeginForm("MonthlyDetailQuery", "Report", FormMethod.Post, new { id = "formMonthlyDetailQuery", @class = "form-horizontal" }))
            {

                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                訂單編號
                            </div>
                            <div class="col-md-3">
                                <input class="form-control" type="text" name="OrderNum" id="OrderNum" value="@ViewData["OrderNum"]" />

                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                會員帳號
                            </div>
                            <div class="col-md-3">
                                <input class="form-control" type="text" name="userID" id="userID" value="@((ViewData["userID"]==null)?"":ViewData["userID"].ToString())">

                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                時數使用起迄日
                            </div>
                            <div class="col-md-3">
                                <!-- datepicker -->
                                <div class="custom-datepicker">
                                    <input class="datePicker form-control" type="text" id="SDate" name="SDate" value="@((ViewData["SDate"]==null)?"":ViewData["SDate"].ToString())" readonly="false">
                                    <svg width="22" height="22" viewBox="0 0 22 22">
                                        <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                            <rect width="22" height="19" rx="3" stroke="none" />
                                            <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                        </g>
                                        <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                            <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                            <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                        </g>
                                        <g transform="translate(3.633 -0.005)">
                                            <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                            <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                        </g>
                                    </svg>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <!-- datepicker with time -->
                                <div class="custom-datepicker">
                                    <input class="datePicker form-control" type="text" id="EDate" name="EDate" value="@((ViewData["EDate"]==null)?"":ViewData["EDate"].ToString())" readonly="false">
                                    <svg width="22" height="22" viewBox="0 0 22 22">
                                        <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                            <rect width="22" height="19" rx="3" stroke="none" />
                                            <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                        </g>
                                        <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                            <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                            <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                        </g>
                                        <g transform="translate(3.633 -0.005)">
                                            <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                            <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                        </g>
                                    </svg>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>

                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-3 offset-md-2">
                                <input type="hidden" id="Account" name="Account" value="@((Session["Account"] == null) ? "" : Session["Account"].ToString())" />
                                <input type="hidden" id="len" name="len" value="@subscriptReportLen" />
                                <button type="submit" class="btn btn-primary">確認送出</button>

                            </div>
                        </div>
                    </div>
                </li>
            }

            @if (subscriptReportLen > 0)
            {
        <li class="list-group-item">
            @using (Html.BeginForm("MonthlyDetailQueryDownload", "Report", FormMethod.Post, new { id = "formMonthlyDetailQueryDownload" }))
            {
                <button id="btnDownload" name="btnDownload" class="btn btn-primary">匯出excel</button>

                <input type="hidden" name="SDate" id="SDate" value="@ViewData["SDate"]">
                <input type="hidden" name="EDate" id="EDate" value="@ViewData["EDate"]">
                <input type="hidden" name="isHandle" id="isHandle" value="@ViewData["isHandle"]">
                <input type="hidden" name="userID" id="userID" value="@ViewData["userID"]">
                <input type="hidden" name="OrderNum" id="OrderNum" value="@ViewData["OrderNum"]">
            }
        </li>
            }
        </ul>





    </div>
    <!--end card-->
</div>
<!--查詢介面結束-->
<!--輸出表格-->
<div class="container mt-4" id="ShowSetCardPanel">

    <table class="table bg-white" data-limit-navigation="5">
        <thead class="text-white bg-primary">
            @if (subscriptReportLen == 0)
            {
                <tr>
                    <th data-sort-inital="descending" data-class="expand">訂單編號</th>
                    <th>IDNO</th>
                    <th>取車據點</th>
                    <th>使用汽車－平日(時)</th>
                    <th>使用汽車－假日(時)</th>
                    <th>使用機車(分)</th>
                    <th>使用時間</th>
                    <th>扣抵訂閱方案編號</th>
                    <th>扣抵方案代碼</th>
                    <th>扣抵方案名稱</th>

                </tr>
            }
            else
            {
                <tr>
                    <th data-sort-inital="descending" data-class="expand">訂單編號</th>
                    <th>IDNO</th>
                    <th>取車據點</th>
                    <th>使用汽車－平日(時)</th>
                    <th>使用汽車－假日(時)</th>
                    <th>使用機車(分)</th>
                    <th>使用時間</th>
                    <th>扣抵訂閱方案編號</th>
                    <th>扣抵方案代碼</th>
                    <th>扣抵方案名稱</th>
                </tr>
            }
        </thead>
        <tbody>
            @if (subscriptReportLen == 0)
            {
                <tr>
                    <td colspan="10" class="text-center">查無結果</td>


                </tr>
            }
            else
            {
                for (int i = 0; i < subscriptReportLen; i++)
                {

                    <tr>
                        <td>@("H" + subscriptReportData[i].OrderNo.ToString().PadLeft(7, '0'))</td>
                        <td> @subscriptReportData[i].IDNO</td>
                        <td> @subscriptReportData[i].lend_place</td>
                        <td>@subscriptReportData[i].UseWorkDayHours</td>

                        <td> @subscriptReportData[i].UseHolidayHours</td>
                        <td> @((subscriptReportData[i].UseMotoTotalHours).ToString("f1"))</td>
                        <td>@subscriptReportData[i].MKTime.ToString("yyyy/MM/dd HH:mm")</td>
                        <td> @subscriptReportData[i].SEQNO</td>
                        <td> @subscriptReportData[i].ProjID</td>
                        <td>@subscriptReportData[i].ProjNM</td>
                    </tr>

                }

            }

        </tbody>
        <tfoot class="hide-if-no-paging">
            <tr>
                <td colspan="10" class="text-center">
                    <ul class="pagination"></ul>
                </td>
            </tr>
        </tfoot>
    </table>
</div>
<div class="modal fade " id="surrounding_modal" tabindex="-1" role="dialog" data-backdrop="static" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <img src="" name="ReviewImg" id="ReviewImg" class="thumbnails" />
            </div>
            <div class="modal-footer">


                <button type="button" class="btn btn-secondary" data-dismiss="modal">CLOSE</button>
            </div>
        </div>
    </div>
</div>
<script language="javascript">

    var SD = "@ViewData["SDate"]";
    var ED = "@ViewData["EDate"]";
    var UserID = "@ViewData["userID"]";
    var OrderNum = "@ViewData["OrderNum"]";
    var outerOfDateRangeMsg = "@ViewData["outerOfDateRangeMsg"]";

</script>

