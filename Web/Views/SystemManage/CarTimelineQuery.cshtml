
@{
    string StationID = (ViewData["StationID"] == null) ? "" : ViewData["StationID"].ToString();
    string CarNo = (ViewData["CarNo"] == null) ? "" : ViewData["CarNo"].ToString();
    string StartDate = (ViewData["StartDate"] == null) ? "" : ViewData["StartDate"].ToString();
    string EndDate = (ViewData["EndDate"] == null) ? "" : ViewData["EndDate"].ToString();
}
<!--查詢介面-->
<div class="container mt-4">
    <!--card start-->
    <div class="card">
        <div class="card-header bg-secondary text-white">
            車輛管理時間查詢
        </div>
        <ul class="list-group list-group-flush">
            <!--每一個li是一項查詢條件-->
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            據點
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="StationID" id="StationID" value="@StationID" />
                            
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            車號
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="CarNo" id="CarNo" value="@CarNo" />
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            日期
                        </div>
                        <div class="col-md-3">
                            <!-- datepicker -->
                            <div class="custom-datepicker">
                                <input class="datePicker form-control" type="text" id="StartDate" name="StartDate" readonly="false" value="@StartDate">
                                <svg width="22" height="22" viewBox="0 0 22 22">
                                    <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                        <rect width="22" height="19" rx="3" stroke="none" />
                                        <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                    </g>
                                    <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                        <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                        <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                    </g>
                                    <g transform="translate(3.633 -0.005)">
                                        <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                        <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                    </g>
                                </svg>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <!-- datepicker with time -->
                            <div class="custom-datepicker">
                                <input class="datePicker form-control" type="text" id="EndDate" name="EndDate" readonly="false" value="@(EndDate)">
                                <svg width="22" height="22" viewBox="0 0 22 22">
                                    <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                        <rect width="22" height="19" rx="3" stroke="none" />
                                        <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                    </g>
                                    <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                        <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                        <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                    </g>
                                    <g transform="translate(3.633 -0.005)">
                                        <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                        <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                    </g>
                                </svg>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-3 offset-md-2">
                            <input type="hidden" id="Account" name="Account" value="@((Session["Account"] == null) ? "" : Session["Account"].ToString())" />
                            <button type="button" id="btnSend" name="btnSend" class="btn btn-primary ">確認送出</button>

                        </div>
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <!--end card-->
</div>
<!--查詢介面結束-->
<div class="container mt-4" id="ShowSetCardPanel">
    <div class="card">
        <div class="row">
            <div class="col-md-12">
                <p>
                    <span class="btn btn-danger">預約中</span>
                    <span class="btn btn-secondary">已取消</span>
                    <span class="btn btn-warning">已取車</span>
                    <span class="btn btn-primary" >延時用車</span>
                    <span class="btn btn-success">已還車</span>
                    <span class="btn " style="background-color:#D5DDF6">清潔/保修</span>
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12" id="visualization" name="visualization">

            </div>
        </div>
    </div>

</div>

