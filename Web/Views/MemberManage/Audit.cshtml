
@model IEnumerable<Domain.WebAPI.output.HiEasyRentAPI.WebAPIOutput_NPR330QueryData>

@{
    string IDNO = (ViewData["IDNO"] == null) ? "" : ViewData["IDNO"].ToString();
    string AuditMode = (ViewData["AuditMode"] == null) ? "0" : ViewData["AuditMode"].ToString();
    string AuditType = (ViewData["AuditType"] == null) ? "0" : ViewData["AuditType"].ToString();
    string AuditReuslt = (ViewData["AuditReuslt"] == null) ? "0" : ViewData["AuditReuslt"].ToString();
    string SDate = (ViewData["StartDate"] == null) ? "" : ViewData["StartDate"].ToString();
    string EDate = (ViewData["EndDate"] == null) ? "" : ViewData["EndDate"].ToString();
    string UserName = (ViewData["UserName"] == null) ? "" : ViewData["UserName"].ToString();

    int ResultLen = -1;
    List<Domain.WebAPI.output.HiEasyRentAPI.WebAPIOutput_NPR330QueryData> lstData = null;
    if (Model != null)
    {
        lstData = new List<Domain.WebAPI.output.HiEasyRentAPI.WebAPIOutput_NPR330QueryData>();
        lstData = Model.ToList();
        ResultLen = lstData.Count;
    }
}
<!--查詢介面-->
<div class="container mt-4">
    <div class="card">
        <div class="card-header bg-secondary text-white">
            會員審核
        </div>
    </div>
</div>
<div class="container mt-4">
    <!--card start-->

    <div class="card">
        <div class="card-header text-white" style="background-color: #9fa4ac;">
            查詢條件
        </div>
        @using (Html.BeginForm("Audit", "MemberManage", FormMethod.Post, new { id = "frmAudit", @class = "form-horizontal", enctype = "multipart/form-data" }))
        {
            <ul class="list-group list-group-flush">
                <!--每一個li是一項查詢條件-->
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                處理項目
                            </div>
                            <div class="col-md-3">
                                <select class="form-control" name="AuditMode" id="AuditMode" value="@AuditMode">
                                    <option value="0"></option>
                                    <option value="1">申請加入</option>
                                    <option value="2">變更身份</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                處理區分
                            </div>
                            <div class="col-md-3">
                                <select class="form-control" name="AuditType" id="AuditType" value="@AuditType">
                                    <option value="0"></option>
                                    <option value="1">未處理</option>
                                    <option value="2">已處理</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                申請日期
                            </div>
                            <div class="col-md-3">
                                <!-- datepicker -->
                                <div class="custom-datepicker">
                                    <input class="datePicker form-control" type="text" id="StartDate" name="StartDate" value="@SDate" readonly="false">
                                    <svg width="22" height="22" viewBox="0 0 22 22">
                                        <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                            <rect width="22" height="19" rx="3" stroke="none" />
                                            <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                        </g>
                                        <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                            <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                            <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                        </g>
                                        <g transform="translate(3.633 -0.005)">
                                            <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                            <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                        </g>
                                    </svg>
                                </div>
                            </div>
                            -
                            <div class="col-md-3">
                                <!-- datepicker with time -->
                                <div class="custom-datepicker">
                                    <input class="datePicker form-control" type="text" id="EndDate" name="EndDate" value="@EDate" readonly="false">
                                    <svg width="22" height="22" viewBox="0 0 22 22">
                                        <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                            <rect width="22" height="19" rx="3" stroke="none" />
                                            <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                        </g>
                                        <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                            <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                            <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                        </g>
                                        <g transform="translate(3.633 -0.005)">
                                            <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                            <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                        </g>
                                    </svg>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                審核結果
                            </div>
                            <div class="col-md-3">
                                <select class="form-control" name="AuditReuslt" id="AuditReuslt" value="@AuditReuslt">
                                    <option value="0"></option>
                                    <option value="1">未通過</option>
                                    <option value="2">已通過</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                姓名
                            </div>
                            <div class="col-md-3">
                                <input class="form-control" type="text" name="UserName" id="UserName" value="@UserName" />

                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                身份證
                            </div>
                            <div class="col-md-3">
                                <input class="form-control" type="text" name="IDNO" id="IDNO" value="@IDNO" />

                            </div>
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-2">
                                身分證尾數
                            </div>
                            @for (int i = 0; i < 10; i++)
                            {
                                <div class="col-md-1">

                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" id="IDNO@(i)" name="IDNOSuff" value="@(i)">
                                        <label class="form-check-label" for="IDNO@(i)">@(i)</label>
                                    </div>


                                </div>
                            }
                        </div>
                    </div>
                </li>
                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-3 offset-md-2">
                                <button type="submit" class="btn btn-info">查詢</button>
                                <button type="reset" class="btn btn-outline-secondary">清除</button>
                                <input type="hidden" id="len" name="len" value="@ResultLen" />
                                <input type="hidden" id="Account" name="Account" value="@((Session["Account"] == null) ? "" : Session["Account"].ToString())" />
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        }
    </div>
    <!--end card-->
</div>
<!--查詢介面結束-->
<div class="container mt-4">
    <div class="card">
        <div class="card-header  text-white" style="background-color: #9fa4ac;">
            查詢結果
        </div>
        </div>
    </div>
    <!--輸出表格-->
    <div class="container mt-4">
        <table class="table bg-white footable" data-filter="#filter" id="list">
            <thead class="text-white bg-primary">
                <tr>
                    <th data-sort="false">

                    </th>
                    <th scope="col">申請日期</th>
                    <th scope="col">姓名</th>
                    <th scope="col">身份證字號</th>
                    <th scope="col">姓別</th>
                    <th scope="col">處理區分</th>
                    <th scope="col">審核結果</th>
                    <th scope="col">審核</th>
                </tr>
            </thead>
            <tbody>
                @if (ResultLen == 0)
                {
                    <tr>
                        <td colspan="8" class="text-center">
                            查無結果
                        </td>
                    </tr>
                }
                else
                {
                    for (int i = 0; i < ResultLen; i++)
                    {
                        <tr>
                            <td data-sort="false">
                            </td>
                            <td scope="col">@(lstData[i].IRENTORDNO)</td>
                            <td scope="col">@(lstData[i].ORDNO)</td>
                            <td scope="col">@(lstData[i].SPAYMENTTYPE)</td>
                            <td scope="col">@(lstData[i].POLNO)</td>
                            <td scope="col">@(lstData[i].DUEAMT)</td>
                            <td scope="col">@(lstData[i].PAIDAMT)</td>
                        </tr>
                    }

                }

            </tbody>
            <tfoot class="hide-if-no-paging">
                <tr>
                    <td colspan="8" class="text-center">
                        <ul class="pagination"></ul>
                    </td>
                </tr>
            </tfoot>
        </table>
    </div>
    <!--輸出表格結束-->

    <script>
    var errorLine = '@((ViewData["errorLine"]==null)?"": ViewData["errorLine"].ToString())';
    var errorMsg = '@((ViewData["errorMsg"] == null)?"" : ViewData["errorMsg"].ToString())';
    </script>
