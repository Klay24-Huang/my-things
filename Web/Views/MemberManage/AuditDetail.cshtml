            if (obj.Images.Business_1_IsNew == 2)
            {
                Business_1_str = "舊系統未同步照片";
            }
            else if (obj.Images.Business_1_IsNew == 0)
            {
                Business_1_str = "已審核";
            }
        }
        MobileLen = (obj.SameMobile == null) ? 0 : obj.SameMobile.Count();
        HistoryLen = (obj.History == null) ? 0 : obj.History.Count();

