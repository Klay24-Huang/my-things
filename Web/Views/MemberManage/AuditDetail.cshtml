    <div id="history_modal" class="modal fade">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <table class="table table-striped bg-white mt-4 " data-limit-navigation="5">
                        <thead class="text-white bg-primary">
                            <tr>
                                <th data-sort="false">修改人員</th>
                                <th scope="col">修改日期</th>
                                <th scope="col">處理事項</th>
                                <th scope="col">處理區分</th>
                                <th scope="col">不通過原因</th>

                            </tr>
                        </thead>
                        <tbody>
                            @if (HistoryLen == 0)
                            {
                                <tr>
                                    <td colspan="5" class="text-center">
                                        查無結果
                                    </td>
                                </tr>
                            }
                            else
                            {
                                for (int i = 0; i < HistoryLen; i++)
                                {
                                    string HandleType = "未處理";
                                    if (obj.History[i].IsReject ==1)
                                    {
                                        HandleType = "已處理，審核未通過";
                                    }
                                    else if(obj.History[i].IsReject !=1)
                                    {
                                        HandleType = "已處理，審核已通過";
                                    }
                                    <tr>
                                        <td data-sort="false">@(obj.History[i].MUserName)</td>
                                        <td scope="col">@(Convert.ToDateTime(obj.History[i].AuditDate).ToString("yyyy-MM-dd HH:mm:ss"))</td>
                                        <td scope="col">@((obj.History[i].HandleItem==0)?"變更身份":"新加入會員")</td>
                                        <td scope="col">@(HandleType)</td>
                                        <td scope="col">@(string.Format("{0}{1}",obj.History[i].RejectExplain,obj.History[i].RejectReason))</td>
                                    </tr>
                                }

                            }

                        </tbody>
                        <tfoot class="hide-if-no-paging">
                            <tr>
                                <td colspan="5" class="text-center">
                                    <ul class="pagination"></ul>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <div class="modal-footer">

                    <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">關閉</button>

                </div>
            </div>
        </div>
    </div>
