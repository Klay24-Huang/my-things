                            </div>
                        </div>
                    </div>
                </li>

                <li class="list-group-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-3 offset-md-2">
                                <button type="button" name="btnExplode" id="btnExplode" class="btn btn-primary">匯出</button>
                                <input type="hidden" name="Mode" id="Mode" value="@(Mode)" />
                                <input type="hidden" id="Account" name="Account" value="@((Session["Account"] == null) ? "" : Session["Account"].ToString())" />
                                <input type="hidden" id="len" name="len" value="@ResultLen" />
                                <button type="submit" class="btn btn-primary">確認送出</button>

                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        }
    </div>
    <!--end card-->
</div>
@using (Html.BeginForm("ContactQueryExplode", "ContactManage", FormMethod.Post, new { id = "formContactQueryExplode" }))
{
    <input type="hidden" id="ExplodeOrderNum" name="ExplodeOrderNum" value="">
    <input type="hidden" id="ExplodeuserID" name="ExplodeuserID" value="">
    <input type="hidden" id="ExplodeobjCar" name="ExplodeobjCar" value="">
    <input type="hidden" id="ExplodeobjStation" name="ExplodeobjStation" value="">
    <input type="hidden" id="ExplodeSDate" name="ExplodeSDate" value="">
    <input type="hidden" id="ExplodeEDate" name="ExplodeEDate" value="">
}
<!--查詢介面結束-->
<!--輸出表格-->
<div class="container mt-4" id="ShowSetCardPanel">
    <table class="table bg-white" data-limit-navigation="5">
        <thead class="text-white bg-primary">
            <tr>
                <th scope="col">預約編號</th>
                <th scope="col">會員帳號</th>
                <th scope="col">取車時間</th>
                <th scope="col">還車時間</th>
                <th scope="col">合約狀態</th>
                <th scope="col">服務據點</th>
                <th scope="col">車號</th>
                <th scope="col">詳細資訊</th>
            </tr>
        </thead>
        <tbody>
            @if (ResultLen == 0)
            {
                <tr>
                    <td colspan="7" class="text-center">查無結果</td>


                </tr>
            }
            else
            {
                for (int i = 0; i < ResultLen; i++)
                {
                    string OrderStatus = "";
                    int isMotor = (lstData[i].CarNo.Substring(0, 1) == "E") ? 1 : 0;
                <tr>
                    <td scope="row">@(string.Format("H{0}", lstData[i].OrderNum.PadLeft(7, '0')))</td>
                    <td scope="col">@(lstData[i].IDNO)</td>
                    <td scope="col">@(lstData[i].FS.ToString("yyyyMMdd HH:mm:ss"))</td>
                    <td scope="col">@(lstData[i].FE.ToString("yyyyMMdd HH:mm:ss"))</td>
                    <td scope="col">
                        @if (lstData[i].cancel_status > 0)
                        {
                            OrderStatus = "已取消";
                        }
                        else
                        {
                            switch (lstData[i].car_mgt_status)
                            {
                                case 0:
                                case 1:
                                case 2:
                                case 3:
                                    OrderStatus = "未取車";
                                    break;
                                case 4:
                                    OrderStatus = "已取車";
                                    break;

                                case 11:
                                    OrderStatus = "已點擊還車";
                                    break;
                                case 15:
                                    OrderStatus = "已付款";
                                    break;
                                case 16:
                                    OrderStatus = "已完成還車";
                                    break;
                            }
                        }
                        @OrderStatus
                    </td>
                    <td scope="col">@(lstData[i].StationName)</td>
                    <td scope="col">@(lstData[i].CarNo)</td>
                    <td scope="col"><button type="button" class="btn btn-outline-secondary" onclick="ViewDetail(@(lstData[i].OrderNum),@(isMotor));">點我觀看</button></td>
                </tr>
                }

            }

        </tbody>
        <tfoot class="hide-if-no-paging">
            <tr>
                <td colspan="7" class="text-center">
                    <ul class="pagination"></ul>
                </td>
            </tr>
        </tfoot>
    </table>
</div>
<form action="" id="showDetail" name="showDetail" method="get">
    <input type="hidden" id="DetailOrderNo" name="DetailOrderNo" />
</form>
<!--輸出表格結束-->

<script>
    var errorLine = '@((ViewData["errorLine"]==null)?"": ViewData["errorLine"].ToString())';
                           var errorMsg = '@((ViewData["errorMsg"] == null)?"" : ViewData["errorMsg"].ToString())';
</script>