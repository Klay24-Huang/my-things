

@{
    string CarNo = (ViewData["CarNo"] == null) ? "" : ViewData["CarNo"].ToString();
    string StationID = (ViewData["StationID"] == null) ? "" : ViewData["StationID"].ToString();
}
<!--查詢介面-->
<div class="container mt-4">
    <!--card start-->
    <div class="card">
        <div class="card-header bg-secondary text-white">
            預約(合約)資料查詢
        </div>
        <ul class="list-group list-group-flush">
            <!--每一個li是一項查詢條件-->
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            預約編號
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="OrderNo" id="OrderNo" value="" />
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            會員帳號
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="IDNO" id="IDNO" value="" />
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            服務據點
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="StationID" id="StationID" value="@StationID" />
                            @Html.Action("GetStationHiddenList", "CommonObj")
                        </div>
                    </div>
                </div>
            </li>
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            車牌號號
                        </div>
                        <div class="col-md-3">
                            <input class="form-control" type="text" name="CarNo" id="CarNo" value="@CarNo" />
                            @Html.Action("GetStationHiddenList", "CommonObj")
                        </div>
                    </div>
                </div>
            </li>


            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-2">
                            訂單起迄日
                        </div>
                        <div class="col-md-3">
                            <!-- datepicker -->
                            <div class="custom-datepicker">
                                <input class="datePicker form-control" type="text" id="StartDate" name="StartDate" readonly="false">
                                <svg width="22" height="22" viewBox="0 0 22 22">
                                    <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                        <rect width="22" height="19" rx="3" stroke="none" />
                                        <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                    </g>
                                    <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                        <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                        <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                    </g>
                                    <g transform="translate(3.633 -0.005)">
                                        <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                        <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                    </g>
                                </svg>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <!-- datepicker with time -->
                            <div class="custom-datepicker">
                                <input class="dateTimePicker form-control" type="text" id="EndDate" name="EndDate" readonly="false">
                                <svg width="22" height="22" viewBox="0 0 22 22">
                                    <g transform="translate(0 3)" fill="#fff" stroke="#9fa4ac" stroke-width="1">
                                        <rect width="22" height="19" rx="3" stroke="none" />
                                        <rect x="0.5" y="0.5" width="21" height="18" rx="2.5" fill="none" />
                                    </g>
                                    <g transform="translate(0 3)" fill="#9fa4ac" stroke="#9fa4ac" stroke-width="1">
                                        <path d="M3,0H19a3,3,0,0,1,3,3V6a0,0,0,0,1,0,0H0A0,0,0,0,1,0,6V3A3,3,0,0,1,3,0Z" stroke="none" />
                                        <path d="M3,.5H19A2.5,2.5,0,0,1,21.5,3V5a.5.5,0,0,1-.5.5H1A.5.5,0,0,1,.5,5V3A2.5,2.5,0,0,1,3,.5Z" fill="none" />
                                    </g>
                                    <g transform="translate(3.633 -0.005)">
                                        <rect id="Rectangle_37" data-name="Rectangle 37" width="2" height="8" rx="1" transform="translate(0.367 0.005)" fill="#9fa4ac" />
                                        <rect id="Rectangle_38" data-name="Rectangle 38" width="2" height="8" rx="1" transform="translate(11.367 0.005)" fill="#9fa4ac" />
                                    </g>
                                </svg>
                            </div>
                        </div>
                    </div>
                </div>
            </li>
 
            <li class="list-group-item">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-3 offset-md-2">
                            <button type="submit" class="btn btn-primary">匯出</button>
                            <button type="submit" class="btn btn-primary">確認送出</button>

                        </div>
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <!--end card-->
</div>
<!--查詢介面結束-->
<!--輸出表格-->
<div class="container mt-4" id="ShowSetCardPanel">
    <table class="table bg-white footable" data-filter="#filter" id="list">
        <thead class="text-white bg-primary">
            <tr>
                <th data-sort="false"></th>
                <th scope="col">預約編號</th>
                <th scope="col">會員帳號</th>
                <th scope="col">合約狀態</th>
                <th scope="col">服務據點</th>
                <th scope="col">車號</th>
                <th scope="col">詳細資訊</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td></td>
                <td scope="row">1</td>
                <td scope="col">1234567890</td>
                <td scope="col"></td>
                <td scope="col"></td>
                <td scope="col"></td>
                <td scope="col"></td>

            </tr>
        </tbody>
    </table>
</div>

<!--輸出表格結束-->
