@model IEnumerable<Domain.TB.BackEnd.BE_GetOrderQueryForWeb>
@{
    string CarNo = (ViewData["CarNo"] == null) ? "" : ViewData["CarNo"].ToString();
    string StationID = (ViewData["StationID"] == null) ? "" : ViewData["StationID"].ToString();
    string IDNO = (ViewData["IDNO"] == null) ? "" : ViewData["IDNO"].ToString();
    string Mode = (ViewData["Mode"] == null) ? "" : ViewData["Mode"].ToString();
    string SDate = (ViewData["SDate"] == null) ? "" : ViewData["SDate"].ToString();
    string EDate = (ViewData["EDate"] == null) ? "" : ViewData["EDate"].ToString();
    string OrderNo = (ViewData["OrderNo"] == null) ? "" : ViewData["OrderNo"].ToString();

    int ResultLen = -1;
    List<Domain.TB.BackEnd.BE_GetOrderQueryForWeb> lstData = null;
    if (Model != null)
    {
        lstData = new List<Domain.TB.BackEnd.BE_GetOrderQueryForWeb>();
        lstData = Model.ToList();
        ResultLen = lstData.Count;
    }
}
<!--查詢介面-->
